<% if(value.isPropertyBase){ -%>
    '<%= key %>' => new <%= value['class'] -%>()
<% } else if(value.isPropertyList) { -%>
    '<%= key %>' => new PropertyList('<%= value['class'] -%>')
<% } else if(value['default'] !== undefined) { -%>
    '<%= key %>' => <%- JSON.stringify(value['default']) -%>
<% } else { -%>
    '<%= key %>' => <%- "" -%>
<% } %>